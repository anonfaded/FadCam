<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3dp" />
            <solid android:color="#BBB6AC" />
            <stroke android:width="0.18dp" android:color="#4A24211D" />
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="2dp" />
            <stroke android:width="0.15dp" android:color="#14FFFFFF" />
        </shape>
    </item>
</layer-list>
